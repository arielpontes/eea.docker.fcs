[unix_http_server]
file = %(here)s/instance/supervisor.sock

[rpcinterface:supervisor]
supervisor.rpcinterface_factory = supervisor.rpcinterface:make_main_rpcinterface

[supervisord]
logfile = %(here)s/instance/supervisor.log
logfile_maxbytes = 50MB
pidfile = %(here)s/instance/supervisor.pid
user = alex
directory = %(here)s

[supervisorctl]
serverurl = unix://%(here)s/instance/supervisor.sock

[program:fcs]
directory=%(here)s/
command=%(here)s/sandbox/bin/waitress-serve --call fcs.app:create_app --port 5010
environment=PYTHONPATH=%(here)s/sandbox/
redirect_stderr = true
stdout_logfile = %(here)s/logs/%(program_name)s.log
stdout_logfile_maxbytes = 10MB
stdout_logfile_backups = 4
startretries = 1
